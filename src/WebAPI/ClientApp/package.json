{
	"name": "plexripper-web-ui",
	"version": "0.23.1",
	"description": "The best cross-platform Plex media downloader there is! In active development and feedback is very welcome!",
	"author": "JasonLandbridge",
	"private": true,
	"bugs": {
		"url": "https://github.com/PlexRipper/PlexRipper/issues"
	},
	"homepage": "https://github.com/PlexRipper/PlexRipper",
	"repository": {
		"type": "git",
		"url": "https://github.com/PlexRipper/PlexRipper.git"
	},
	"scripts": {
		"dev": "nuxi dev",
		"build": "nuxi build",
		"generate": "nuxi generate",
		"preview": "nuxi preview",
		"dev:analyze": "nuxi analyze",
		"postinstall": "nuxi prepare",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --sort-types --modular --clean-output --module-name-index 1 --axios --api-class-name GeneratedApiClient --templates ./src/types/api/templates/modular -o ./src/types/api/generated",
		"typecheck": "vue-tsc --noEmit --pretty",
		"typecheck-watch": "vue-tsc --noEmit --watch --pretty",
		"npm-upgrade-packages": "npm-upgrade",
		"test": "vitest --config ./vitest.config.mts run",
		"test-watch": "vitest --config ./vitest.config.mts watch",
		"cypress:e2e": "start-server-and-test 'nuxi dev --port 3030 --dotenv .env.cypress' http://localhost:3030 'npx cypress open --e2e --browser firefox'",
		"cypress:ci": "start-server-and-test 'nuxi dev --port 3030 --dotenv .env.cypress' http://localhost:3030 'npx cypress run'",
		"eslint-config-inspector": "npx @eslint/config-inspector",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"@codewithdan/observable-store": "^2.2.15",
		"@microsoft/signalr": "^7.0.7",
		"@pinia/nuxt": "^0.5.1",
		"@primevue/themes": "^4.0.1",
		"@vueuse/rxjs": "^10.11.0",
		"axios": "^1.7.2",
		"caniuse-lite": "^1.0.30001664",
		"consola": "^3.2.3",
		"date-fns": "^2.30.0",
		"pinia": "^2.1.7",
		"pretty-bytes": "^6.1.1",
		"primevue": "~4.0.1",
		"quasar": "^2.16.6",
		"query-string": "^9.1.0",
		"rxjs": "^7.8.1",
		"vue": "^3.4.34",
		"vue-virtual-scroller": "^2.0.0-beta.8",
		"vue3-otp-input": "^0.5.21"
	},
	"devDependencies": {
		"@hirez_io/observer-spy": "^2.2.0",
		"@intlify/eslint-plugin-vue-i18n": "^3.0.0",
		"@ngneat/falso": "^7.2.0",
		"@nuxt/eslint": "^0.4.0",
		"@nuxt/test-utils": "^3.13.1",
		"@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@primevue/nuxt-module": "^4.0.1",
		"@quasar/extras": "^1.16.12",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@testing-library/cypress": "^10.0.2",
		"@testing-library/dom": "^10.4.0",
		"@types/lodash-es": "^4.17.12",
		"@types/mocha": "^10.0.7",
		"@types/node": "^20.14.12",
		"@vueuse/core": "^10.11.0",
		"@vueuse/nuxt": "^10.11.0",
		"axios-mock-adapter": "^1.22.0",
		"cypress": "^13.13.1",
		"cypress-signalr-mock": "^1.5.0",
		"cypress-vite": "^1.5.0",
		"happy-dom": "^14.12.3",
		"nuxt": "3.12.4",
		"nuxt-lodash": "^2.5.3",
		"nuxt-quasar-ui": "^2.1.3",
		"sass": "^1.77.8",
		"sass-loader": "^15.0.0",
		"semantic-release": "^24.0.0",
		"semantic-release-export-data": "^1.1.0",
		"start-server-and-test": "^2.0.4",
		"swagger-typescript-api": "^13.0.13",
		"typescript": "^5.5.4",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.4",
		"vitest-environment-nuxt": "^1.0.0",
		"vue-tsc": "^2.0.29"
	}
}
